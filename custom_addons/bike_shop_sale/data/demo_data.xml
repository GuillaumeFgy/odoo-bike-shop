        <record id="demo_sale_order_1" model="sale.order">
            <field name="partner_id" ref="bike_shop_rental.demo_partner_1"/>
            <field name="date_order" eval="(datetime.now() - timedelta(days=5)).strftime('%Y-%m-%d')"/>
            <field name="state">sale</field>
        </record>

        <record id="demo_sale_order_line_1_1" model="sale.order.line">
            <field name="order_id" ref="demo_sale_order_1"/>
            <field name="product_id" ref="product_bike_trek_city"/>
            <field name="product_uom_qty">1</field>
        </record>

